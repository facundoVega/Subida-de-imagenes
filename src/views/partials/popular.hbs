<div class="card mb-2">
    <div class="card-header">
        <h5><i class="fas fa-fire"></i> Most popular</h5>
    </div>
    <div class="card-body">
        <div class="row">
            {{#each sidebar.images}}
            <div class="col-md-4">
                <a href="/images/{{uniqueId}}" >
                <img src="/public/upload/{{filename}}" class="w-100 h-100 img-thumbnail">
                </a>
            </div>
            {{/each}}
        </div>
    </div>
</div>